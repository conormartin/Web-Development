<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport">
    <meta http-equiv="X-UA-Compatible" content="ie=edge">
    <title>Search Results</title>
</head>
<body>
    <h1>Search Results:</h1>
    <ul>
        <% data["Search"].forEach(function(movie){ %>
            <li>
                <strong><%= movie["Title"] %> </strong>- <%= movie["Year"] %>
            </li>
            <% }); %>
    </ul>

    <a href="/">Search Again!</a>
</body>
</html>